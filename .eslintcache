paign}\n      campaigns={campaign.campaigns}\n      pagination={article.pagination}\n      getArtcsByFaculty={getArticlesByFacultyAndStatus}\n      userDetails={user.details}\n    />\n  );\n};\n\nHomePage.propTypes = {\n  auth: PropTypes.object.isRequired,\n  article: PropTypes.object.isRequired,\n  getArticlesByFacultyAndStatus: PropTypes.func.isRequired,\n  getArticlesByFacultyAndStatusAndCampaign: PropTypes.func.isRequired,\n  getCampaigns: PropTypes.func.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n  article: state.article,\n  campaign: state.campaign,\n});\n\nexport default connect(mapStateToProps, {\n  getArticlesByFacultyAndStatus,\n  getArticlesByFacultyAndStatusAndCampaign,\n  getCampaigns,\n})(HomePage);\n","/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Routing/PrivateRoute.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Alert/Alert.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/views/CampaignPage/CampaignPage.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/views/ArticlePage/CreateArticlePage.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/views/ArticlePage/AllArticlesPage.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/redux/actions/auth.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/api/axios.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/redux/actions/types.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/redux/actions/alert.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/redux/actions/campaign.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Campaign/CampaignBreadcrumbs.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Article/CreateArticleBreadcrumbs.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Campaign/CreateCampaignForm.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/src/components/Article/ArticleBreadcrumbs.js",[],"/Users/phmthsn11/Desktop/COMP-1640/frontend/